<view><block wx:for="{{tabber_list}}" wx:for-item="item" wx:for-index="i" wx:key="i"><view><block wx:if="{{item.iconsList.length>0&&item.title_show}}"><view class="cu-bar bg-white solid-top "><view class="action"><text class="{{['cuIcon-title','','text-'+item.color]}}"></text>{{''+item.title+''}}</view></view></block><block wx:if="{{item.iconsList.length>0}}"><view class="{{['cu-list','grid','no-border','solid-bottom','','col-'+item.colNumber+' '+item.bgColor]}}" data-cur="{{item.cur}}"><block wx:for="{{item.iconsList}}" wx:for-item="icon" wx:for-index="index" wx:key="index"><block wx:if="{{item.iconsList.length<=item.colNumber*2||item.iconsList.length>item.colNumber*2&&index<item.colNumber*2-1}}"><view class="cu-item " data-url="{{icon.url}}" data-exec="{{icon.exec}}" data-event-opts="{{[['tap',[['navigateTo',['$event']]]]]}}" bindtap="__e"><view class="{{['cuIcon-'+icon.cuIcon,'text-'+icon.color]}}"></view><text>{{icon.name}}</text></view></block></block><block wx:for="{{item.iconsList}}" wx:for-item="icon" wx:for-index="index" wx:key="index"><block wx:if="{{item.iconsList.length>item.colNumber*2&&index>=item.colNumber*2-1&&item.showAll}}"><view class="cu-item " data-url="{{icon.url}}" data-event-opts="{{[['tap',[['navigateTo',['$event']]]]]}}" bindtap="__e"><view class="{{['cuIcon-'+icon.cuIcon,'text-'+icon.color]}}"></view><text>{{icon.name}}</text></view></block></block><block wx:if="{{item.iconsList.length>item.colNumber*2}}"><view class="cu-item" data-cur="{{item.cur}}" data-event-opts="{{[['tap',[['showAllGrid',['$event']]]]]}}" bindtap="__e"><view class="{{['','text-blue',item.showAll?'cuIcon-fold':'cuIcon-apps']}}"></view><text>{{item.showAll?'折叠':'全部'}}</text></view></block></view></block></view></block></view>
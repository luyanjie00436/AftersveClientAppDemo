xt-df" name="RepairStart"><text class="title text-bold">{{i18n_product.device.repairStart}}</text><uni-dateformat class="text-grey" vue-id="71f84c94-4" format="yyyy-MM-dd" date="{{current.RepairStart}}" bind:__l="__l"></uni-dateformat></view><block wx:if="{{false}}"><view class="cu-form-group text-df" name="RepairEnd"><text class="title text-bold">{{i18n_product.device.repairEnd}}</text><uni-dateformat class="text-grey" vue-id="71f84c94-5" format="yyyy-MM-dd" date="{{current.RepairEnd}}" bind:__l="__l"></uni-dateformat></view></block><block wx:if="{{false}}"><view class="cu-form-group text-df" name="MchName"><text class="title text-bold">{{i18n_product.device.mchId}}</text><text class="text-grey">{{current.MchName}}</text></view></block><block wx:if="{{false}}"><view class="cu-form-group text-df" name="Name"><text class="title text-bold">{{i18n_product.device.address}}</text><text class="text-grey">{{current.Address}}</text></view></block><block wx:if="{{false}}"><view class="cu-form-group text-df" name="SupplierMchName"><text class="title text-bold">{{i18n_product.device.supplier}}</text><text class="text-grey">{{''+current.SupplierMchName}}</text></view></block></view>